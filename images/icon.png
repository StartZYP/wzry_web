<script type="text/javascript" src="/images/jquery.min.js"></script>
<script type="text/javascript" src="/images/layer.js"></script>
<script type="text/javascript" src="/images/H-ui.min.js"></script>
<script type="text/javascript" src="/images/jquery.validate.js"></script>
<script type="text/javascript" src="/images/common.js"></script>
<script type="text/javascript">
    $(function(){
        $("#myform").validate({
            submitHandler:function(form){
                if ($("#father").val() ==0){
                    layer.msg('请选择服务器');
                    return false;
                }

                if ($("#userAccount").val()==""){
                    layer.msg('请输入账号');
                    return false;
                }
                if ($("#exchangeCDKeyValue").val()==""){
                    layer.msg('请输入激活码');
                    return false;
                }

                var exchangeCDKeyValue = $.trim($('#exchangeCDKeyValue').val());
                var userAccount = $.trim($('#userAccount').val());
                var father = $.trim($('#father').val());
                var verifyCode = $.trim($('#verifyCode').val());

                    $.ajax({
                    'url': '/duihuan',
                    'type': 'POST',
                    // 'contentType': "application/json",
                    'data': {
                        userAccount: userAccount,
                        exchangeCDKeyValue: exchangeCDKeyValue,
                        roleName: father
                    },
                    'dataType': 'json',
                    'success': function (result) {
                        var code = result.code;
                        var data = result.data;
                        alert(result.msg);
                    },
                    'error': function (re, EX) {
                       alert(re.responseText);
                    }
                });
            }
        });
    });
</script>

</body></html>